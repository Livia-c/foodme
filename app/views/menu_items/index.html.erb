    <br>
    <%= link_to "Add new menu item", new_menu_item_path, class: "btn btn-primary m-4" %>
  </div>
  <ul>
    <div class="menu-item-card">
      <%= render "item_card", menu_items: @menu_items %>
    </div>
  </ul>
</div>

